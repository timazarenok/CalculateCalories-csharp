<Window x:Class="CaloriesCalculator.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CaloriesCalculator"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="Ingredients" Content="Ингредиенты" Width="150" Height="30" Click="Ingredients_Click"/>
            <Button x:Name="Categories" Content="Категории" Width="150" Height="30" Click="Categories_Click"/>
            <Button x:Name="Dishes" Content="Блюда" Width="150" Height="30" Click="Dishes_Click"/>
            <Button x:Name="DishesIngredients" Width="150" Height="30" Content="Ингредиенты в блюдах" Click="DishesIngredients_Click"/>
        </StackPanel>
        <Button x:Name="Exit" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="120" Height="30" Margin="30,0,0,30" Click="Exit_Click">Выход</Button>
    </Grid>
</Window>
